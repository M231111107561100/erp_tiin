{
  "ConnectionStrings": {
    "Default": "Host=db;Port=${POSTGRES_PORT};Database=${POSTGRES_DB};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD}"
  },
  "Jwt": {
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "JwksUri": "${JWT_JWKS_URI}"
  },
  "Redis": {
    "Host": "${REDIS_HOST}",
    "Port": "${REDIS_PORT}",
    "Password": "${REDIS_PASSWORD}"
  },
  "Minio": {
    "Endpoint": "${MINIO_ENDPOINT}",
    "AccessKey": "${MINIO_ROOT_USER}",
    "SecretKey": "${MINIO_ROOT_PASSWORD}",
    "Bucket": "${S3_BUCKET}",
    "UseSsl": "${S3_USE_SSL}"
  },
  "Otlp": {
    "Endpoint": "${OTEL_EXPORTER_OTLP_ENDPOINT}"
  }
}
